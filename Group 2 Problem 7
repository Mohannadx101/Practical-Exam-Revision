#include <iostream>

using namespace std;

int main() {
	double number;

	cout << "Enter a real number: ";
	cin >> number;


	if (number > 0) {
		cout << "The number is POSITIVE!!" << endl;
	}
	else if (number < 0) {
		cout << "The number is NEGATIVE!!" << endl;
	}
	else {
		cout << "The number is EQUAL to ZERO!!" << endl;
	}

	return 0;
}
